</view>

<view wx:if="{{progress==1}}"
  class="padding progress animation-{{progressAnimation}} {{loading==false?'animation-reverse':''}}"
  style="--ScreenHeight16Percent:{{ScreenHeight16Percent}}px;">
  <view class="cu-progress radius striped active">
    <view class="bg-blue" style="width:{{water+'%'}};">{{water}}</view>
  </view>
  <view class="cu-progress radius striped active margin-top">
    <view class="bg-orange" style="width:{{fertilizer+'%'}};">{{fertilizer}}</view>
  </view>
  <view class="cu-progress radius striped active margin-top">
    <view class="bg-green" style="width:{{medicine+'%'}};">{{medicine}}</view>
  </view>
</view>

<view class="questionButton text-xsl" style="--ScreenHeight10Percent:{{ScreenHeight10Percent}}px;" bindtap="showModal">
